i) import pandas as pd
# Create a Pandas Series from a list
data = [10, 20, 30, 40, 50]
series = pd.Series(data)
# Display the Series
print("Pandas Series:")
print(series)

ii) # Convert the Pandas Series to a Python list
python_list = series.tolist()
# Display the list and its type
print("\nConverted Python list:", python_list)
print("Type of the converted object:", type(python_list))
