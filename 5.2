import matplotlib.pyplot as plt
# Sample data
x = [1, 2, 3, 4, 5]
y_scatter = [5, 7, 4, 6, 8]
y_line = [1, 3, 2, 5, 4]
categories = ['A', 'B', 'C', 'D', 'E']
values = [3, 7, 5, 6, 9]
# Create subplots
fig, axs = plt.subplots(1, 3, figsize=(15, 4))
# Scatter Plot
axs[0].scatter(x, y_scatter, color='blue')
axs[0].set_title("Scatter Plot")
axs[0].set_xlabel("X-axis")
axs[0].set_ylabel("Y-axis")
# Line Plot
axs[1].plot(x, y_line, marker='o', color='green')
axs[1].set_title("Line Plot")
axs[1].set_xlabel("X-axis")
axs[1].set_ylabel("Y-axis")
# Bar Chart
axs[2].bar(categories, values, color='orange')
axs[2].set_title("Bar Chart")
axs[2].set_xlabel("Category")
axs[2].set_ylabel("Value")
# Layout adjustment
plt.tight_layout()
plt.show()
